<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.nhom8.csdl.Employee" table="employee">
		<id name="employeeId" column="EMP_ID" type="long">
			<generator class="native" />
		</id>
		<property name="employeeName" column="EMP_NAME" type="string" />
		<property name="employeeHireDate" column="EMP_HIRE_DATE"
			type="date" />
		<property name="employeeSalary" column="EMP_SALARY" type="double" />
	</class>
</hibernate-mapping>